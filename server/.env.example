# Copy this file to .env and fill values before deploying or running locally
# MongoDB Atlas connection string (example):
# MONGO_URI=mongodb+srv://<username>:<password>@cluster0.abcd.mongodb.net/riantblog?retryWrites=true&w=majority
MONGO_URI=

# Optional: port to listen on (Render will set its own PORT env var)
PORT=5000

# Frontend origin (for CORS). When deploying, put the Vercel URL here, e.g. https://your-frontend.vercel.app
CLIENT_ORIGIN=http://localhost:3000
